<table class="table table-hover">
  <thead>
    <tr class="info">
      <th><%= t "admin.user.table.id" %></th>
      <th><%= t "admin.user.table.name" %></th>
      <th><%= t "admin.user.table.email" %></th>
      <th><%= t "admin.user.table.create_at" %></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= link_to user.name, "#" %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td>
          <%= link_to t("admin.product.table.edit"), "#",
            class: "btn btn-primary" %>
          <%= link_to t("admin.product.table.delete"), "#" ,
            method: :delete, data: {confirm: t("messages.confirm")}, class: "btn btn-danger" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate %>